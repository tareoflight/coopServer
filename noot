#!/bin/sh

case $1 in
    --generate)
        exec spacetime generate --lang csharp --module-name noot --out-dir generate
        ;;
    --publish)
        exec spacetime publish noot
        ;;
    *)
        VERB=$1
        if [ ! -z "$VERB" ]; then
            shift
            exec spacetime $VERB noot "$@"
        fi
        exec spacetime
        ;;
esac